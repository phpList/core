{
  "name": "phplist/phplist4-core",
  "description": "The world's most popular open source newsletter manager",
  "keywords": [
    "phplist",
    "email",
    "newsletter",
    "manager"
  ],
  "homepage": "http://www.phpList.com/",
  "type": "project",
  "license": "GNU Affero General Public License version 3.0 or later (AGPLv3+)",
  "support": {
    "issues": "https://github.com/phpList/phpList/issues",
    "forum": "http://forums.phplist.com/",
    "wiki": "http://resources.phplist.com/",
    "source": "https://github.com/phpList/phpList"
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">= 7.0.0, <= 7.1.99",
    "symfony/dependency-injection": "~2.6",
    "symfony/yaml": "~2.6",
    "symfony/config": "~2.6",
    "phpmailer/phpmailer": "v5.2.7",
    "psr/log": "1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "3.7.*",
    "squizlabs/php_codesniffer": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "phpList\\": "core/",
      "phpList\\helper\\": "core/helper/"
    },
    "files": [
      "core/helper/Shortcuts.php"
    ]
  }
}
